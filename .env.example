# Flask Configuration
DEBUG=True
HOST=0.0.0.0
PORT=5000

# Seedr Client Configuration
SEEDR_TIMEOUT=30.0
SEEDR_PROXY=

# Token Storage
TOKEN_STORAGE_PATH=tokens.json

# Seedr Default Credentials
# Copy this file to .env and fill in your actual credentials
DEFAULT_USERNAME=your_email@example.com
